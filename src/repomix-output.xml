<file path="public/css/frontend-custom.css">
/* ==========================================
   SYNWAVECO FRONTEND CUSTOM CSS OVERRIDES
   Ghi đè theme Cartzilla với GreenTech IoT Identity
   =========================================== */

:root {
    /* Import CSS Variables từ layout.css */
    /* Primary Colors - Dark Greens */
    --primary-super-dark: #008B45;  /* SpringGreen4  */
    --primary-dark: #3CB371;        /* MediumSpringGreen */
    --primary-medium: #2E8B57;      /* SeaGreen */
    
    /* Primary Colors - Light Greens */
    --primary-light: #00CD66;       /* SpringGreen3 */
    --primary-lighter: #98FB98;     /* PaleGreen */
    --primary-pale: #90EE90;        /* PaleGreen2 */
    
    /* Vibrant Accent Greens */
    --green-spring: #00FF7F;        /* SpringGreen1 */
    --green-spring2: #00EE76;       /* SpringGreen2 */
    --green-lime: #32CD32;          /* LimeGreen */
    --green-aqua: #7FFFD4;          /* Aquamarine1 */
    --green-sea-light: #20B2AA;     /* LightSeaGreen */
    --green-lawn: #7CFC00;          /* LawnGreen */
    --green-medium-spring: #00FA9A; /* MediumSpringGreen */
    
    /* Secondary Colors - Blue Accent */
    --accent-dark: #104E8B;         /* DodgerBlue4 */
    --accent-medium: #1E90FF;       /* DodgerBlue1 */
    --accent-light: #4682B4;        /* SteelBlue */
    --accent-lighter: #B0E0E6;      /* PowderBlue */
    
    /* Light Mode Text Colors */
    --text-dark: #000000;           /* Black */
    --text-medium: #696969;         /* DimGray */
    --text-light: #D3D3D3;          /* LightGray */
    --text-green: #2E8B57;          /* SeaGreen */
    --text-green-dark: #008B45;     /* SpringGreen4 */
    
    /* Backgrounds */
    --bg-light: #F5F5F5;            /* WhiteSmoke */
    --bg-primary: #f8f9fa;          /* Light background */
}

/* ==========================================
   1. BODY & GENERAL OVERRIDES
   =========================================== */
body {
    background-color: var(--bg-primary) !important;
    color: var(--text-dark) !important;
}

/* ==========================================
   2. NAVBAR CUSTOMIZATION - Gradient Green
   =========================================== */
.navbar {
    background: linear-gradient(135deg, 
        var(--primary-super-dark) 0%, 
        var(--primary-dark) 25%, 
        var(--primary-medium) 50%, 
        var(--primary-light) 75%, 
        var(--green-lime) 100%) !important;
    box-shadow: 0 4px 20px rgba(0, 100, 0, 0.4);
    border-bottom: 2px solid var(--green-spring);
}

.navbar-brand {
    font-weight: bold;
    font-size: 1.3rem;
    display: flex;
    align-items: center;
    gap: 10px;
    text-shadow: 0 2px 4px rgba(0, 0, 0, 0.3);
}

.navbar-brand i {
    color: var(--green-spring);
    filter: drop-shadow(0 0 3px rgba(0, 255, 127, 0.5));
}

.navbar .nav-link {
    color: rgba(255, 255, 255, 0.95) !important;
    font-weight: 500;
    transition: all 0.3s ease;
    padding: 0.5rem 1rem;
    text-shadow: 0 1px 2px rgba(0, 0, 0, 0.2);
}

.navbar .nav-link:hover {
    color: white !important;
    background: linear-gradient(135deg, rgba(0, 255, 127, 0.3) 0%, rgba(152, 251, 152, 0.2) 100%);
    border-radius: 6px;
    transform: translateY(-1px);
}

.navbar .dropdown-menu {
    background: linear-gradient(180deg, var(--primary-lighter) 0%, var(--primary-pale) 50%, #FFFFFF 100%);
    border: 2px solid var(--primary-light);
    box-shadow: 0 10px 30px rgba(0, 100, 0, 0.25);
    border-radius: 10px;
}

.navbar .dropdown-item {
    color: var(--primary-super-dark);
    transition: all 0.3s ease;
    padding: 0.7rem 1rem;
    font-weight: 500;
}

.navbar .dropdown-item:hover {
    background: linear-gradient(90deg, var(--primary-dark) 0%, var(--primary-medium) 50%, var(--primary-light) 100%);
    color: white;
}

.navbar-toggler {
    border: 2px solid var(--green-spring);
    color: white;
    background: rgba(0, 255, 127, 0.1);
}


/* ==========================================
   3. BUTTONS - GreenTech Theme
   =========================================== */

/* Primary Button */
.btn-primary {
    background: linear-gradient(135deg, var(--primary-super-dark) 0%, var(--primary-dark) 100%);
    border-color: var(--primary-dark);
    color: white;
    font-weight: 600;
    transition: all 0.3s ease;
}

.btn-primary:hover {
    background: linear-gradient(135deg, var(--primary-dark) 0%, var(--primary-medium) 100%);
    border-color: var(--primary-medium);
    transform: translateY(-2px);
    box-shadow: 0 8px 20px rgba(0, 139, 69, 0.4);
}

.btn-primary:active,
.btn-primary.active {
    background: linear-gradient(135deg, var(--primary-super-dark) 0%, var(--primary-dark) 100%);
    border-color: var(--primary-dark);
}

/* Secondary Button */
.btn-secondary {
    background-color: var(--primary-light);
    border-color: var(--primary-light);
    color: var(--primary-super-dark);
    font-weight: 600;
}

.btn-secondary:hover {
    background-color: var(--primary-dark);
    border-color: var(--primary-dark);
    color: white;
    box-shadow: 0 8px 20px rgba(60, 179, 113, 0.4);
}

/* Outline Button */
.btn-outline-primary {
    color: var(--primary-super-dark);
    border: 2px solid var(--primary-super-dark);
}

.btn-outline-primary:hover {
    background: var(--primary-super-dark);
    color: white;
    border-color: var(--primary-super-dark);
    box-shadow: 0 8px 20px rgba(0, 139, 69, 0.4);
}

/* Danger Button */
.btn-danger {
    background-color: #FF3030;
    border-color: #FF3030;
}

.btn-danger:hover {
    background-color: #E60000;
    border-color: #E60000;
    box-shadow: 0 8px 20px rgba(255, 48, 48, 0.4);
}

/* Success Button */
.btn-success {
    background-color: var(--green-lime);
    border-color: var(--green-lime);
    color: white;
}

.btn-success:hover {
    background-color: var(--primary-light);
    border-color: var(--primary-light);
    box-shadow: 0 8px 20px rgba(50, 205, 50, 0.4);
}

/* Button Icon */
.btn-icon {
    transition: all 0.3s ease;
}

.btn-icon:hover {
    transform: scale(1.1);
}

/* ==========================================
   4. PRODUCT CARDS - Shadow & Hover Effects
   =========================================== */

.product-card {
    transition: all 0.3s ease;
    background-color: #f8f9fa !important;
    border: 1px solid #0aebeb;
    border-radius: 12px;
}

.product-card:hover {
    box-shadow: 0 12px 40px rgba(0, 139, 69, 0.25);
    transform: translateY(-4px);
}

.product-card .badge {
    background: linear-gradient(135deg, var(--green-spring) 0%, var(--primary-light) 100%);
    color: var(--primary-super-dark);
    font-weight: bold;
}

.product-card-button {
    background: linear-gradient(135deg, var(--primary-super-dark) 0%, var(--primary-dark) 100%);
    border: none;
    color: white;
}

.product-card-button:hover {
    background: linear-gradient(135deg, var(--primary-dark) 0%, var(--primary-medium) 100%);
    box-shadow: 0 8px 20px rgba(0, 139, 69, 0.4);
}

/* ==========================================
   5. SEARCH FORM - GreenTech Styling
   =========================================== */

.search-form .form-control {
    border-radius: 25px 0 0 25px;
    border: 2px solid var(--primary-light);
    background-color: rgba(184, 237, 182, 0.95); !important;
    padding-left: 1rem;
    font-size: 0.95rem;
}

.search-form .form-control:focus {
    border-color: var(--green-spring);
    box-shadow: 0 0 0 4px rgba(0, 255, 127, 0.25);
}

.search-form .btn {
    border-radius: 0 25px 25px 0;
    border: 2px solid var(--primary-light);
    border-left: none;
    background: linear-gradient(135deg, var(--primary-medium) 0%, var(--primary-light) 50%, var(--green-lime) 100%);
    color: white;
    padding: 0.375rem 1rem;
    font-weight: 600;
}

.search-form .btn:hover {
    background: linear-gradient(135deg, var(--green-lime) 0%, var(--green-spring) 50%, var(--primary-lighter) 100%);
    color: var(--primary-super-dark);
}

/* ==========================================
   6. CARDS & SECTIONS - Enhanced Styling
   =========================================== */

.card {
    background-color: white !important;
    border: 1px solid #e8e8e8;
    border-radius: 12px;
    transition: all 0.3s ease;
}

.card:hover {
    box-shadow: 0 8px 30px rgba(0, 139, 69, 0.15);
}

.card-header {
    background: linear-gradient(90deg, var(--primary-lighter) 0%, var(--primary-pale) 100%);
    border-bottom: 2px solid var(--primary-light);
    color: var(--primary-super-dark);
    font-weight: 600;
}

/* ==========================================
   7. FOOTER - Dark Blue Accent
   =========================================== */

footer {
    background: linear-gradient(135deg, var(--accent-dark) 0%, #0D3A66 50%, var(--primary-super-dark) 100%);
    color: white;
    border-top: 3px solid var(--green-spring);
}

footer h5 {
    color: var(--green-spring);
    font-weight: bold;
}

footer a {
    color: rgba(255, 255, 255, 0.9);
    transition: all 0.3s ease;
}

footer a:hover {
    color: var(--green-spring);
    text-shadow: 0 0 10px rgba(0, 255, 127, 0.5);
}

/* ==========================================
   8. FORMS & INPUTS - GreenTech Focus State
   =========================================== */

.form-control,
.form-select {
    border: 1px solid #ddd;
    border-radius: 8px;
    transition: all 0.3s ease;
}

.form-control:focus,
.form-select:focus {
    border-color: var(--primary-light);
    box-shadow: 0 0 0 4px rgba(0, 205, 102, 0.1);
}

.form-label {
    color: var(--text-dark);
    font-weight: 600;
    margin-bottom: 0.5rem;
}

/* ==========================================
   9. BADGES & ALERTS - GreenTech Colors
   =========================================== */

.badge-primary {
    background-color: var(--primary-super-dark);
}

.badge-info {
    background: linear-gradient(135deg, var(--green-spring) 0%, var(--primary-light) 100%);
    color: var(--primary-super-dark);
    font-weight: bold;
}

.badge-success {
    background-color: var(--green-lime);
    color: white;
}

.alert-success {
    background-color: rgba(50, 205, 50, 0.1);
    border-color: var(--green-lime);
    color: var(--primary-super-dark);
}

.alert-info {
    background-color: rgba(0, 205, 102, 0.1);
    border-color: var(--primary-light);
    color: var(--primary-super-dark);
}

.alert-warning {
    background-color: rgba(255, 165, 0, 0.1);
    border-color: #FFA500;
    color: #664D00;
}

.alert-danger {
    background-color: rgba(255, 48, 48, 0.1);
    border-color: #FF3030;
    color: #990000;
}

/* ==========================================
   10. OFFCANVAS & MODAL - GreenTech Styling
   =========================================== */

.offcanvas {
    background-color: #f8f9fa;
    z-index: 10050 !important;
}

.offcanvas-backdrop {
    z-index: 10040 !important;
}

.offcanvas-header {
    border-bottom: 2px solid var(--primary-light);
    background-color: rgba(0, 139, 69, 0.05);
}

.offcanvas-title {
    color: var(--primary-super-dark);
    font-weight: bold;
}

.modal-header {
    border-bottom: 2px solid var(--primary-light);
    background: linear-gradient(90deg, var(--primary-lighter) 0%, var(--primary-pale) 100%);
}

.modal-title {
    color: var(--primary-super-dark);
    font-weight: bold;
}

.modal-body {
    background-color: #f8f9fa;
}


#searchBox.offcanvas-top {
    height: auto !important;
    min-height: 180px;
    border-bottom: 2px solid var(--primary-light);
    box-shadow: 0 10px 30px rgba(0,0,0,0.2) !important;
}

#shoppingCart.offcanvas-end {
    border-left: 1px solid var(--primary-light);
}
/* ==========================================
   11. BREADCRUMB - Navigation Styling
   =========================================== */

.breadcrumb {
    background-color: transparent;
    gap: 0.5rem;
}

.breadcrumb-item.active {
    color: var(--primary-super-dark);
    font-weight: 600;
}

.breadcrumb-item a {
    color: var(--primary-light);
    text-decoration: none;
    transition: all 0.3s ease;
}

.breadcrumb-item a:hover {
    color: var(--primary-super-dark);
}

/* ==========================================
   12. PAGINATION - Navigation Style
   =========================================== */

.pagination .page-link {
    color: var(--primary-super-dark);
    border: 1px solid #ddd;
    background-color: white;
}

.pagination .page-link:hover {
    color: white;
    background: linear-gradient(135deg, var(--primary-super-dark) 0%, var(--primary-dark) 100%);
    border-color: var(--primary-dark);
}

.pagination .page-item.active .page-link {
    background: linear-gradient(135deg, var(--primary-super-dark) 0%, var(--primary-dark) 100%);
    border-color: var(--primary-dark);
}

/* ==========================================
   13. TABLES - Enhanced Styling
   =========================================== */

.table {
    border-collapse: collapse;
}

.table thead th {
    background: linear-gradient(90deg, var(--primary-lighter) 0%, var(--primary-pale) 100%);
    color: var(--primary-super-dark);
    font-weight: 600;
    border-bottom: 2px solid var(--primary-light);
}

.table tbody tr {
    transition: all 0.3s ease;
}

.table tbody tr:hover {
    background-color: rgba(0, 139, 69, 0.05);
}

/* ==========================================
   14. SPINNER & LOADING - GreenTech Animation
   =========================================== */

.spinner-border {
    color: var(--primary-super-dark);
}

.spinner-border-sm {
    width: 1rem;
    height: 1rem;
}

.loading-skeleton {
    background: linear-gradient(90deg, #f0f0f0 25%, #e0e0e0 50%, #f0f0f0 75%);
    background-size: 200% 100%;
    animation: loading-pulse 1.5s infinite;
}

@keyframes loading-pulse {
    0% {
        background-position: 200% 0;
    }
    100% {
        background-position: -200% 0;
    }
}

/* ==========================================
   15. HERO & BANNER SECTIONS
   =========================================== */

.hero-section {
    background: linear-gradient(135deg, 
        var(--primary-super-dark) 0%, 
        var(--primary-dark) 25%, 
        var(--primary-medium) 50%, 
        var(--primary-light) 75%, 
        var(--green-lime) 100%);
    color: white;
    padding: 4rem 2rem;
    border-radius: 12px;
}

.hero-section h1 {
    text-shadow: 0 2px 10px rgba(0, 0, 0, 0.3);
    font-weight: bold;
}

.hero-section p {
    font-size: 1.1rem;
    opacity: 0.95;
}

/* ==========================================
   16. TAGS & LABELS - GreenTech Theme
   =========================================== */

.label {
    background-color: var(--primary-super-dark);
    color: white;
    padding: 0.3rem 0.8rem;
    border-radius: 4px;
    font-weight: 600;
    display: inline-block;
    font-size: 0.85rem;
}

.label-info {
    background: linear-gradient(135deg, var(--green-spring) 0%, var(--primary-light) 100%);
    color: var(--primary-super-dark);
}

.label-success {
    background-color: var(--green-lime);
    color: white;
}

.label-warning {
    background-color: #FFA500;
    color: white;
}

.label-danger {
    background-color: #FF3030;
    color: white;
}

/* ==========================================
   17. SWIPER & CAROUSEL ENHANCEMENTS
   =========================================== */

.swiper-pagination-bullet-active {
    background-color: var(--primary-super-dark);
}

.swiper-button-next,
.swiper-button-prev {
    color: var(--primary-super-dark);
}

.swiper-button-next:hover,
.swiper-button-prev:hover {
    color: var(--primary-dark);
}

/* ==========================================
   18. LINK & ANCHOR STYLES
   =========================================== */

a {
    color: var(--primary-super-dark);
    text-decoration: none;
    transition: all 0.3s ease;
}

a:hover {
    color: var(--primary-dark);
    text-decoration: underline;
}

/* ==========================================
   19. TEXT & UTILITY CLASSES
   =========================================== */

.text-primary {
    color: var(--primary-super-dark) !important;
}

.text-success {
    color: var(--green-lime) !important;
}

.text-info {
    color: var(--primary-light) !important;
}

.text-danger {
    color: #FF3030 !important;
}

.bg-primary {
    background-color: var(--primary-super-dark) !important;
}

.bg-success {
    background-color: var(--green-lime) !important;
}

.bg-info {
    background: linear-gradient(135deg, var(--green-spring) 0%, var(--primary-light) 100%) !important;
}

.border-primary {
    border-color: var(--primary-super-dark) !important;
}

.border-success {
    border-color: var(--green-lime) !important;
}

/* ==========================================
   20. RESPONSIVE ADJUSTMENTS
   =========================================== */

@media (max-width: 768px) {
    .navbar {
        padding: 0.5rem 0;
    }
    
    .navbar-brand {
        font-size: 1.1rem;
    }
    
    .nav-link {
        padding: 0.5rem 0.5rem !important;
    }
    
    .product-card {
        margin-bottom: 1rem;
    }
    
    .hero-section {
        padding: 2rem 1rem;
    }
    
    .btn {
        font-size: 0.9rem;
        padding: 0.5rem 0.8rem;
    }
}

@media (max-width: 576px) {
    .navbar-brand {
        font-size: 1rem;
    }
    
    .btn-primary,
    .btn-secondary {
        font-size: 0.85rem;
        padding: 0.4rem 0.6rem;
    }
    
    .product-card {
        padding: 0.75rem;
    }
}

/* ==========================================
   21. DARK MODE SUPPORT (Optional)
   =========================================== */

@media (prefers-color-scheme: dark) {
    body {
        background-color: #1a1a1a;
        color: #e8e8e8;
    }
    
    .card,
    .product-card {
        background-color: #ebfff2 !important;
        border-color: #3a3a3a;
    }
    
    .form-control,
    .form-select {
        background-color: #f8f9fa;
        color: #030104; 
        border-color: #3a3a3a;
    }
    
    
    .form-control:focus,
    .form-select:focus {
        background-color: #c9ebcf;
        color: #030104;
    }
}

/* ==========================================
   22. PRINT STYLES
   =========================================== */

@media print {
    .navbar,
    footer,
    .btn,
    .offcanvas {
        display: none;
    }
    
    body {
        background-color: white;
    }
    
    .card,
    .product-card {
        border: 1px solid #1df112;
        page-break-inside: avoid;
    }
}


@media (prefers-reduced-motion: reduce) {
    *,
    *::before,
    *::after {
        animation-duration: 0.01ms !important;
        animation-iteration-count: 1 !important;
        transition-duration: 0.01ms !important;
    }
}

@media (prefers-contrast: high) {
    .btn,
    .card,
    .product-card {
        border-width: 2px;
    }
}

/* Focus visible for better keyboard navigation */
*:focus-visible {
    outline: 2px solid var(--green-spring);
    outline-offset: 2px;
}

/* ==========================================
   24. CUSTOM SCROLLBAR (Optional)
   =========================================== */

::-webkit-scrollbar {
    width: 10px;
    height: 10px;
}

::-webkit-scrollbar-track {
    background: #f1f1f1;
}

::-webkit-scrollbar-thumb {
    background: linear-gradient(180deg, var(--primary-light) 0%, var(--green-lime) 100%);
    border-radius: 5px;
}

::-webkit-scrollbar-thumb:hover {
    background: var(--green-spring);
}


/* 1. Thiết lập cho Submenu đa cấp */
.dropdown-submenu {
    position: relative;
}

.dropdown-submenu > .dropdown-menu {
    top: 0;
    left: 100%; /* Đẩy submenu sang phải trên desktop */
    margin-top: -6px;
    margin-left: -1px;
    display: none; /* Mặc định ẩn */
    border-radius: 8px;
    background: white;
    border: 1px solid var(--primary-light);
    min-width: 200px;
    z-index: 10002;
}

/* 2. Hiển thị khi Hover */
.dropdown-submenu:hover > .dropdown-menu {
    display: block;
    animation: slideInLeft 0.2s ease-out;
}

/* Hiệu ứng chuyển cảnh mượt mà */
@keyframes slideInLeft {
    from { opacity: 0; transform: translateX(10px); }
    to { opacity: 1; transform: translateX(0); }
}

/* 3. Tinh chỉnh màu sắc cho đồng nhất SynWavEco */
.dropdown-item:hover {
    background-color: var(--primary-pale) !important;
    color: var(--primary-super-dark) !important;
}

/* 4. Xử lý trên Mobile (Ẩn hover, dùng click nếu cần) */
@media (max-width: 991.98px) {
    .dropdown-submenu > .dropdown-menu {
        position: static;
        left: 0;
        margin-left: 20px;
        border: none;
        box-shadow: none;
        background: transparent;
        display: none;
    }
    
    .dropdown-submenu.show > .dropdown-menu {
        display: block;
    }
}
