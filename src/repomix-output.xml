<file path="public/css/form.css">
/* Agricultural IoT Green Theme with Light & Dark mode */

:root {
    /* Light Mode Colors */
    --modal-bg: #ffffff;
    --modal-header-bg: linear-gradient(135deg, #e8f5e9 0%, #c8e6c9 100%);
    --modal-border: #81c784;
    --modal-shadow: rgba(34, 139, 34, 0.15);
    --text-primary: #1b4332;
    --text-secondary: #2d6a4f;
    --text-muted: #52796f;
    --input-bg: #ffffff;
    --input-border: #a5d6a7;
    --input-focus-border: #228b22;
    --input-focus-shadow: rgba(34, 139, 34, 0.25);
    --label-color: #1b4332;
    --accent-primary: #228b22;
    --accent-hover: #32cd32;
    --accent-success: #2e7d32;
    --accent-danger: #c62828;
    --accent-warning: #f9a825;
    --btn-cancel-bg: #6c757d;
    --btn-cancel-hover: #545b62;
}

/* Item Modal Styles */
.item-modal,
.delete-modal .modal-content {
    background: var(--modal-bg);
    border: 2px solid var(--modal-border);
    border-radius: 1rem;
    box-shadow: 0 1rem 3rem var(--modal-shadow);
    transition: all 0.3s ease;
    overflow: hidden;
}

.item-modal-header {
    background: linear-gradient(
        135deg,
        #006400 0%,
        #228b22 25%,
        #2e8b57 50%,
        #3cb371 75%,
        #32cd32 100%
    );
    border-bottom: 3px solid #00ff7f;
    padding: 1.5rem 2rem;
    position: relative;
}

.item-modal-header,
.delete-modal .modal-header {
    background: linear-gradient(
        135deg,
        #006400 0%,
        #228b22 25%,
        #2e8b57 50%,
        #3cb371 75%,
        #32cd32 100%
    );
    border-bottom: 3px solid #00ff7f;
    padding: 1.5rem 2rem;
    position: relative;
}

.item-modal-header .modal-title,
.delete-modal .modal-title {
    color: #ffffff;
    font-size: 1.5rem;
    font-weight: 700;
    display: flex;
    align-items: center;
    gap: 0.75rem;
    margin: 0;
    text-shadow: 1px 1px 2px rgba(0, 0, 0, 0.3);
}

.item-modal-header .modal-title i,
.delete-modal .modal-title i {
    color: #00ff7f;
    font-size: 1.3rem;
}

.delete-modal .modal-title i.text-warning {
    color: var(--accent-warning) !important;
}

.item-modal-header .btn-close,
.delete-modal .btn-close {
    background: transparent;
    border: none;
    font-size: 1.2rem;
    color: #ffffff;
    opacity: 0.8;
    transition: all 0.3s ease;
    padding: 0.5rem;
    border-radius: 50%;
    width: 2.5rem;
    height: 2.5rem;
    display: flex;
    align-items: center;
    justify-content: center;
    filter: brightness(0) invert(1);
}

.item-modal-header .btn-close:hover,
.delete-modal .btn-close:hover {
    opacity: 1;
    background: rgba(255, 255, 255, 0.2);
    transform: scale(1.1);
}

/* Modal Body */
.item-modal .modal-body {
    padding: 2rem;
    max-height: 60vh;
    overflow-y: auto;
}

/* Form Groups */
.form-group {
    margin-bottom: 1.5rem;
}

.form-label {
    display: flex;
    align-items: center;
    gap: 0.5rem;
    font-weight: 600;
    color: var(--label-color);
    margin-bottom: 0.75rem;
    font-size: 1rem;
    white-space: nowrap;
    overflow: hidden;
}

.form-check-input {
    width: 1.5rem;
    height: 1.5rem;
    border: 2px solid var(--input-border);
    border-radius: 0.3rem;
    transition: all 0.3s ease;
    margin-top: 0.2rem;
}

.form-check-input:checked {
    background-color: var(--accent-primary);
    border-color: var(--accent-primary);
    box-shadow: 0 0 0 0.2rem var(--input-focus-shadow);
    white-space: nowrap;
    background-image: url("data:image/svg+xml,%3csvg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 20 20'%3e%3cpath fill='none' stroke='%23fff' stroke-linecap='round' stroke-linejoin='round' stroke-width='3' d='m6 10 3 3 6-6'/%3e%3c/svg%3e");
}

.form-label i {
    /*color: var(--accent-primary);*/
    color: var(--text-secondary);
    font-size: 1rem;
    width: 1.2rem;
    text-align: center;
    transition: color 0.3s ease;
    white-space: nowrap;
}

/* Input Styles */
.item-input,
.item-textarea,
.form-select.item-input {
    width: 100%;
    padding: 0.875rem 1rem;
    border: 2px solid var(--input-border);
    border-radius: 0.5rem;
    background: var(--input-bg);
    color: var(--text-primary);
    font-size: 1rem;
    transition: all 0.3s ease;
    font-family: "Segoe UI", Tahoma, Geneva, Verdana, sans-serif;
}

.item-input:focus,
.item-textarea:focus,
.form-select.item-input:focus {
    outline: none;
    border-color: var(--input-focus-border);
    box-shadow: 0 0 0 0.2rem var(--input-focus-shadow);
    background: var(--input-bg);
}

.item-input::placeholder,
.item-textarea::placeholder {
    color: var(--text-muted);
    opacity: 0.8;
}

.item-textarea {
    resize: vertical;
    min-height: 100px;
    line-height: 1.5;
}

/* Validation Styles */
.item-input.is-invalid,
.item-textarea.is-invalid {
    border-color: var(--accent-danger);
    box-shadow: 0 0 0 0.2rem rgba(198, 40, 40, 0.25);
}

.invalid-feedback {
    display: block;
    color: var(--accent-danger);
    font-size: 0.875rem;
    margin-top: 0.5rem;
    font-weight: 500;
}

/* Alert Styles */
.alert {
    padding: 1rem 1.5rem;
    border-radius: 0.5rem;
    margin-bottom: 1rem;
    font-weight: 500;
    display: flex;
    align-items: center;
    gap: 0.5rem;
}

.alert-danger {
    background: rgba(198, 40, 40, 0.1);
    border: 1px solid rgba(198, 40, 40, 0.3);
    color: var(--accent-danger);
}

.alert-success {
    background: rgba(46, 125, 50, 0.1);
    border: 1px solid rgba(46, 125, 50, 0.3);
    color: var(--accent-success);
}

/* Modal Footer */
.item-modal-footer,
.delete-modal .modal-footer {
    background: linear-gradient(135deg, #c8e6c9 0%, #e8f5e9 50%, #a5d6a7 100%);
    border-top: 3px solid #00ff7f;
    padding: 1.5rem 2rem;
    display: flex;
    justify-content: flex-end;
    gap: 1rem;
}

/* Button Styles */
.btn-cancel {
    background-color: #6c757d;
    color: #ffffff;
    border: solid 2px #6c757d;
    padding: 10px 20px;
    border-radius: 8px;
    font-weight: 500;
    transition: all 0.2s ease;
}

.btn-cancel:hover {
    background-color: #545b62;
    border-color: #228b22;
    color: #ffffff;
}

.btn-action {
    background: linear-gradient(135deg, #006400 0%, #228b22 100%);
    border: 2px solid #00ff7f;
    color: #ffffff;
    padding: 0.75rem 1.5rem;
    border-radius: 0.5rem;
    font-weight: 600;
    display: inline-flex;
    align-items: center;
    gap: 0.5rem;
    transition: all 0.3s ease;
    font-size: 1rem;
    cursor: pointer;
    min-width: 150px;
    justify-content: center;
}

.btn-action:hover:not(:disabled) {
    background: linear-gradient(135deg, #228b22 0%, #32cd32 100%);
    border-color: #00ff7f;
    color: #ffffff;
    transform: translateY(-2px);
    box-shadow: 0 6px 20px rgba(0, 255, 127, 0.4);
}

.btn-action:disabled {
    opacity: 0.7;
    cursor: not-allowed;
    transform: none;
    box-shadow: none;
}

.btn-action .fa-spinner {
    animation: spin 1s linear infinite;
}

.btn-delete {
    background: linear-gradient(135deg, #b91c1c, #991b1b);
    color: white;
    border: solid 2px #ff6b6b;
    padding: 10px 20px;
    border-radius: 8px;
    font-weight: 500;
    text-decoration: none;
