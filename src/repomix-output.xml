}

/* Primary Green Gradient */
.btn-gt-primary {
    background: linear-gradient(135deg, #006400 0%, #228b22 100%);
    border: 2px solid #00ff7f;
    color: #ffffff;
}

.btn-gt-primary:hover {
    background: linear-gradient(135deg, #228b22 0%, #32cd32 100%);
    box-shadow: 0 6px 20px rgba(0, 255, 127, 0.4);
    color: white;
    transform: translateY(-2px);
}

.btn-gt-secondary {
    background: linear-gradient(135deg, #080064 0%, #3b228b 100%);
    border: 2px solid #2600ff;
    color: #ffffff;
}

.btn-gt-secondary:hover {
    background: linear-gradient(135deg, #22228b 0%, #3232cd 100%);
    box-shadow: 0 6px 20px rgba(0, 13, 255, 0.4);
    color: white;
    transform: translateY(-2px);
}

/* Blue Import Gradient */
.btn-gt-import {
    background: linear-gradient(135deg, #1565c0 0%, #42a5f5 100%);
    border: 2px solid #64b5f6;
    color: white;
}

/* Purple Export Gradient */
.btn-gt-export {
    background: linear-gradient(135deg, #7b1fa2 0%, #ce93d8 100%);
    border: 2px solid #e1bee7;
