    border: solid 2px #ff3030;
    text-decoration: none;
    transform: translateY(-1px);
    box-shadow: 0 4px 12px rgba(220, 38, 38, 0.3);
}

.btn-import {
    background: linear-gradient(135deg, #1565c0 0%, #42a5f5 100%);
