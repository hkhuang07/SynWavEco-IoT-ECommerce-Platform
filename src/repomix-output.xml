<file path="public/css/layout.css">
/* ===================================
   GREENTECH IoT AGRICULTURE THEME
   Enhanced Gradient with Dark/Light Green Contrast
   Updated with NewsMart Auth Styles
   =================================== */

:root {
    /* Primary Colors - Dark Greens */
    --primary-super-dark: #008B45;  /* SpringGreen4  */
    --primary-dark: #3CB371;        /* MediumSpringGreen */
    --primary-medium: #2E8B57;      /* SeaGreen */
    
    /* Primary Colors - Light Greens */
    --primary-light: #00CD66;       /* SpringGreen3 */
    --primary-lighter: #98FB98;     /* PaleGreen */
    --primary-pale: #90EE90;        /* PaleGreen2 */
    
    /* Vibrant Accent Greens */
    --green-spring: #00FF7F;        /* SpringGreen1 */
    --green-spring2: #00EE76;       /* SpringGreen2 */
    --green-lime: #32CD32;          /* LimeGreen */
    --green-aqua: #7FFFD4;          /* Aquamarine1 */
    --green-sea-light: #20B2AA;     /* LightSeaGreen */
    --green-lawn: #7CFC00;          /* LawnGreen */
    --green-medium-spring: #00FA9A; /* MediumSpringGreen */
    
    /* Secondary Colors - Blue Accent */
    --accent-dark: #104E8B;         /* DodgerBlue4 */
    --accent-medium: #1E90FF;       /* DodgerBlue1 */
    --accent-light: #4682B4;        /* SteelBlue */
    --accent-lighter: #B0E0E6;      /* PowderBlue */
    
    /* Light Mode Text Colors */
    --text-dark: #000000;           /* Black */
    --text-medium: #696969;         /* DimGray */
    --text-light: #D3D3D3;          /* LightGray */
    --text-green: #2E8B57;          /* SeaGreen */
    --text-green-dark: #008B45;     /* SpringGreen4 */
    
    /* Dark Mode Text Colors */
    --text-dm-primary: #E6E6FA;     /* Lavender */
    --text-dm-secondary: #F0F8FF;   /* AliceBlue */
    --text-dm-muted: #C6E2FF;       /* SlateGray1 */
    --text-dm-ghost: #F8F8FF;       /* GhostWhite */
    --text-dm-azure: #F0FFFF;       /* Azure1 */
    
    /* Backgrounds */
    --bg-light: #F5F5F5;            /* WhiteSmoke */
    --bg-dark: #1C1C1C;             /* Gray11 */
    --bg-darker: #0A1F0A;
    --bg-darkest: #041504;

    --bg-orangeyellow: #FFD700;    /* Gold */
    --bg-orangered: #FF4500;      /* OrangeRed */

    /* Auth Colors for NewsMart Style Integration */
    --auth-primary: #228B22;
    --auth-secondary: #2E8B57;
    --auth-dark: #008B45;
    --auth-input-bg: rgba(0, 0, 0, 0.9);
    --auth-border: #32CD32;
    --auth-focus: #00FF7F;
    --auth-error: #FF3030;
    --auth-success: #00CD00;
    --auth-text: #F0FFF0;
    --auth-text-light: #E8F5E9;
    --auth-bg-overlay: rgba(0, 0, 0, 0.9);
    --auth-card-bg: rgba(255, 255, 255, 0.1);
    --auth-card-border: rgba(255, 255, 255, 0.2);
    --auth-shadow: 0 8px 32px 0 rgba(34, 139, 34, 0.37);
}

/* ===================================
   BODY AND GENERAL STYLES
   =================================== */
body {
    background-color: var(--bg-light);
    font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
    color: var(--text-dark);
}   

/* ===================================
   NAVBAR HORIZONTAL - HIGH CONTRAST GRADIENT
   =================================== */
.navbar-custom {
    background: linear-gradient(135deg, 
        var(--primary-super-dark) 0%, 
        var(--primary-dark) 25%, 
        var(--primary-medium) 50%, 
        var(--primary-light) 75%, 
        var(--green-lime) 100%);
    box-shadow: 0 4px 20px rgba(0, 100, 0, 0.4);
    min-height: 60px;
    white-space: nowrap;
    border-bottom: 2px solid var(--green-spring);
}

.navbar-custom .navbar-brand {
    color: white !important;
    font-weight: bold;
    font-size: 1.3rem;
    display: flex;
    align-items: center;
    gap: 10px;
    text-shadow: 0 2px 4px rgba(0, 0, 0, 0.3);
}

.navbar-custom .navbar-brand i {
    color: var(--green-spring);
    filter: drop-shadow(0 0 3px rgba(0, 255, 127, 0.5));
}

.navbar-custom .navbar-nav .nav-link {
    color: rgba(255, 255, 255, 0.95) !important;
    font-weight: 500;
    transition: all 0.3s ease;
    padding: 0.5rem 1rem;
    text-shadow: 0 1px 2px rgba(0, 0, 0, 0.2);
}

.navbar-custom .navbar-nav .nav-link:hover {
    color: white !important;
    background: linear-gradient(135deg, rgba(0, 255, 127, 0.3) 0%, rgba(152, 251, 152, 0.2) 100%);
    border-radius: 6px;
    transform: translateY(-1px);
}

.navbar-custom .dropdown-menu {
    background: linear-gradient(180deg, var(--primary-lighter) 0%, var(--primary-pale) 50%, #FFFFFF 100%);
    border: 2px solid var(--primary-light);
    box-shadow: 0 10px 30px rgba(0, 100, 0, 0.25);
    border-radius: 10px;
}

.navbar-custom .dropdown-item {
    color: var(--primary-super-dark);
    transition: all 0.3s ease;
    padding: 0.7rem 1rem;
    font-weight: 500;
}

.navbar-custom .dropdown-item:hover {
    background: linear-gradient(90deg, var(--primary-dark) 0%, var(--primary-medium) 50%, var(--primary-light) 100%);
    color: white;
}

.navbar-custom .navbar-toggler {
    border: 2px solid var(--green-spring);
    color: white;
    background: rgba(0, 255, 127, 0.1);
}

/* Brand Text */
.brand-text {
    font-size: 1.25rem;
    text-shadow: #7FFFD4 0 0 5px;
    color: #030104;
    font-weight: 600;
}

/* ===================================
   SIDEBAR TOGGLE BUTTON
   =================================== */
.sidebar-toggle {
    background: linear-gradient(135deg, rgba(0, 255, 127, 0.2) 0%, rgba(152, 251, 152, 0.1) 100%);
    border: 2px solid var(--green-spring);
    color: white;
    font-size: 1.2rem;
    margin-right: 1rem;
    padding: 0.4rem 0.8rem;
    border-radius: 8px;
    transition: all 0.3s ease;
}

.sidebar-toggle:hover {
    background: linear-gradient(135deg, var(--green-spring) 0%, var(--primary-lighter) 100%);
    color: var(--primary-super-dark);
    transform: scale(1.05);
}

/* ===================================
   SEARCH FORM
   =================================== */
.search-form {
    max-width: 350px;
    margin: 0 auto;
}

.search-form .form-control {
    border-radius: 25px 0 0 25px;
    border: 2px solid var(--primary-light);
    background-color: rgba(255, 255, 255, 0.95);
    padding-left: 1rem;
}

.search-form .form-control:focus {
    border-color: var(--green-spring);
    box-shadow: 0 0 0 4px rgba(0, 255, 127, 0.25);
}

.search-form .btn {
    border-radius: 0 25px 25px 0;
    border: 2px solid var(--primary-light);
    border-left: none;
    background: linear-gradient(135deg, var(--primary-medium) 0%, var(--primary-light) 50%, var(--green-lime) 100%);
    color: white;
    padding: 0.375rem 1rem;
}

.search-form .btn:hover {
    background: linear-gradient(135deg, var(--green-lime) 0%, var(--green-spring) 50%, var(--primary-lighter) 100%);
    color: var(--primary-super-dark);
}

/* ===================================
   SIDEBAR STYLES - ENHANCED GRADIENT
   =================================== */
.sidebar {
    position: fixed;
    top: 0;
    left: -400px;
    width: 400px;
    height: 100vh;
    z-index: 1050;
    background: linear-gradient(180deg, 
        var(--primary-super-dark) 0%, 
        var(--primary-dark) 30%, 
        var(--primary-medium) 60%, 
        var(--bg-darker) 100%);
    transition: all 0.35s cubic-bezier(0.4, 0, 0.2, 1);
    overflow-y: auto;
    box-shadow: 4px 0 25px rgba(0, 100, 0, 0.4);
    border-right: 3px solid var(--green-spring);
}

.sidebar.show {
    left: 0;
}

.sidebar .sidebar-nav {
    padding-top: 65px; 
}

.sidebar-header {
    padding: 1.2rem 1rem;
    background: linear-gradient(135deg, 
        var(--primary-light) 0%, 
        var(--primary-medium) 50%, 
        var(--primary-dark) 100%);
    border-bottom: 3px solid var(--green-spring);
}

.sidebar-header h5 {
    color: white;
    margin: 0;
    font-weight: bold;
    display: flex;
    align-items: center;
    justify-content: space-between;
    text-shadow: 0 2px 4px rgba(0, 0, 0, 0.3);
}

.sidebar-header h5 i {
    color: var(--green-spring);
    margin-right: 8px;
    filter: drop-shadow(0 0 4px rgba(0, 255, 127, 0.6));
}

.sidebar-close {
    background: linear-gradient(135deg, rgba(255, 255, 255, 0.2) 0%, rgba(0, 255, 127, 0.2) 100%);
    border: 1px solid var(--green-spring);
    color: white;
    font-size: 1.1rem;
    width: 34px;
    height: 34px;
    border-radius: 50%;
    transition: all 0.3s ease;
    display: flex;
    align-items: center;
    justify-content: center;
}

.sidebar-close:hover {
    background: linear-gradient(135deg, var(--green-spring) 0%, var(--primary-lighter) 100%);
    color: var(--primary-super-dark);
    transform: rotate(90deg);
}

.sidebar-nav {
    padding: 0;
    margin: 0;
    list-style: none;
}

.sidebar-nav .nav-item {
    border-bottom: 1px solid rgba(0, 255, 127, 0.15);
}

.sidebar-nav .nav-link {
    display: block;
    padding: 0.8rem 1.2rem;
    color: rgba(255, 255, 255, 0.9);
    text-decoration: none;
    transition: all 0.3s ease;
    font-weight: 500;
}

.sidebar-nav .nav-link:hover {
    background: linear-gradient(90deg, 
        rgba(0, 255, 127, 0.25) 0%, 
        rgba(152, 251, 152, 0.15) 50%, 
        transparent 100%);
    color: var(--green-spring);
    padding-left: 1.5rem;
    border-left: 3px solid var(--green-spring);
}

.sidebar-nav .nav-link i {
    width: 25px;
    margin-right: 12px;
    color: var(--primary-light);
}

.sidebar-nav .nav-link:hover i {
    color: var(--green-spring);
    filter: drop-shadow(0 0 3px rgba(0, 255, 127, 0.5));
}

/* ===================================
   SIDEBAR SECTIONS
   =================================== */
.sidebar-section-title {
    color: var(--green-aqua);
    font-size: 0.8rem;
    font-weight: bold;
    text-transform: uppercase;
    padding: 1rem 1.2rem 0.5rem;
    margin: 0;
    letter-spacing: 1px;
    border-top: 1px solid rgba(127, 255, 212, 0.25);
    background: linear-gradient(90deg, rgba(0, 100, 0, 0.3) 0%, transparent 100%);
}

/* ===================================
   SIDEBAR DROPDOWNS
   =================================== */
.sidebar-dropdown {
    background: none;
    border: none;
    width: 100%;
    text-align: left;
    padding: 0.8rem 1.2rem;
    color: rgba(255, 255, 255, 0.9);
    transition: all 0.3s ease;
    font-weight: 500;
    cursor: pointer;
}

.sidebar-dropdown:hover {
    background: linear-gradient(90deg, 
        rgba(0, 255, 127, 0.2) 0%, 
        rgba(152, 251, 152, 0.1) 50%, 
        transparent 100%);
    color: var(--green-spring);
}

.sidebar-dropdown i {
    width: 22px;
    margin-right: 12px;
    color: var(--primary-lighter);
}

.sidebar-dropdown:hover i {
    color: var(--green-spring);
}

.sidebar-dropdown .dropdown-icon {
    float: right;
    transition: transform 0.3s ease;
    color: var(--primary-lighter);
}

.sidebar-dropdown.collapsed .dropdown-icon {
    transform: rotate(-90deg);
}

.sidebar-submenu {
    background: linear-gradient(180deg, rgba(0, 100, 0, 0.3) 0%, rgba(0, 0, 0, 0.25) 100%);
    max-height: 0;
    overflow: hidden;
    transition: max-height 0.3s ease;
}

.sidebar-submenu.show {
    max-height: 500px;
}

.sidebar-submenu .nav-link {
    padding-left: 3.5rem;
    font-size: 0.9rem;
    color: rgba(255, 255, 255, 0.75);
    border-left: 3px solid transparent;
}

.sidebar-submenu .nav-link:hover {
    color: var(--green-spring);
    background: linear-gradient(90deg, rgba(0, 255, 127, 0.15) 0%, transparent 100%);
    border-left-color: var(--green-spring);
}

/* ===================================
   OVERLAY
   =================================== */
.sidebar-overlay {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: linear-gradient(135deg, rgba(0, 100, 0, 0.5) 0%, rgba(0, 0, 0, 0.6) 100%);
    z-index: 1040;
    opacity: 0;
    visibility: hidden;
    transition: all 0.3s ease;
    backdrop-filter: blur(3px);
}

.sidebar-overlay.show {
    opacity: 1;
    visibility: visible;
}

/* ===================================
   THEME AND LANGUAGE TOGGLES
   =================================== */
.theme-toggle,
.lang-toggle {
    background: none;
    border: none;
    color: rgba(255, 255, 255, 0.9);
    padding: 0.8rem 1.2rem;
    width: 100%;
    text-align: left;
    transition: all 0.3s ease;
    cursor: pointer;
    font-weight: 500;
}

.theme-toggle i,
.lang-toggle i {
    width: 22px;
    margin-right: 12px;
    color: var(--primary-lighter);
}

.theme-toggle:hover,
.lang-toggle:hover {
    background: linear-gradient(90deg, rgba(0, 255, 127, 0.2) 0%, transparent 100%);
    color: var(--green-spring);
}

.theme-toggle:hover i,
.lang-toggle:hover i {
    color: var(--green-spring);
}

.modal-backdrop {
    z-index: 1059 !important; /* Ngay dưới Modal */
}

/* Đảm bảo Modal nằm trên Backdrop */
.modal {
    z-index: 1060 !important; /* Cao hơn mọi thứ: Navbar (1030), Sidebar (1050), Backdrop (1059) */
}

/* ===================================
   BADGE STYLES
   =================================== */
.badge-role {
    background: linear-gradient(135deg, var(--green-spring) 0%, var(--green-lime) 50%, var(--primary-light) 100%);
    color: var(--primary-super-dark);
    font-size: 0.7rem;
    padding: 0.25rem 0.6rem;
    border-radius: 12px;
    margin-left: 0.5rem;
    font-weight: 600;
    box-shadow: 0 2px 6px rgba(0, 255, 127, 0.3);
}

/* ===================================
   MAIN CONTENT
   =================================== */
.main-content {
    transition: margin-left 0.35s cubic-bezier(0.4, 0, 0.2, 1);
    min-height: calc(100vh - 200px);
}

.main-content.sidebar-open {
    margin-left: 400px;
}

@media (max-width: 768px) {
    .main-content.sidebar-open {
        margin-left: 0;
    }
}

/* ===================================
   FOOTER - FULL DESIGN WITH HIGH CONTRAST
   =================================== */
footer {
    background: linear-gradient(135deg, 
        var(--bg-dark) 0%, 
        var(--primary-medium) 50%, 
        var(--bg-darker) 100%);
    color: var(--text-dm-primary);
    margin-top: 2rem;
    padding: 0;
    border-top: 4px solid;
    border-image: linear-gradient(90deg, var(--green-spring) 0%, var(--green-lime) 50%, var(--primary-lighter) 100%) 1;
}

.footer-service-intro {
    background: linear-gradient(135deg, 
        rgba(0, 100, 0, 0.253) 0%, 
        rgba(0, 255, 127, 0.15) 50%, 
        rgba(46, 139, 87, 0.3) 100%);
    border: 2px solid var(--primary-light);
    box-shadow: 0 10px 40px rgba(0, 100, 0, 0.3);
}

.footer-service-intro h2 {
    color: var(--green-spring);
    text-shadow: 0 2px 4px rgba(0, 0, 0, 0.3);
}

.footer-service-intro h2 i {
    color: var(--green-aqua);
    filter: drop-shadow(0 0 5px rgba(127, 255, 212, 0.5));
}

.footer-service-intro p {
    color: var(--text-dm-secondary);
    line-height: 1.7;
}

.footer-service-intro .highlight {
