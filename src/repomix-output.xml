}
/* ====== DARK MODE ====== */
.dark-mode {
    --modal-bg: #1a2e1a;
    --modal-header-bg: linear-gradient(
        90deg,
        #002200 0%,
        #006400 50%,
        #002200 100%
    );
    --modal-border: #00ff7f;
    --modal-shadow: rgba(0, 255, 127, 0.2);
    --text-primary: #e8f5e9;
    --text-secondary: #a5d6a7;
    --text-muted: #81c784;
    --input-bg: #2a3e2a;
    --input-border: #2e7d32;
    --input-focus-border: #00ff7f;
    --input-focus-shadow: rgba(0, 255, 127, 0.25);
    --label-color: #a5d6a7;
    --accent-primary: #00ff7f;
    --accent-hover: #32cd32;
    --accent-success: #4caf50;
    --accent-danger: #f87171;
    --btn-cancel-bg: #4a4a4a;
    --btn-cancel-hover: #6a6a6a;
}

.dark-mode .item-modal,
.dark-mode .delete-modal .modal-content {
    background: var(--modal-bg);
    border: 2px solid var(--modal-border);
    box-shadow: 0 1rem 3rem rgba(0, 255, 127, 0.2);
}

.dark-mode .item-modal-header,
.dark-mode .delete-modal .modal-header {
    background: linear-gradient(
        135deg,
        #002200 0%,
        #004d00 25%,
        #006400 50%,
        #228b22 75%,
        #32cd32 100%
    );
    border-bottom: 3px solid #00ff7f;
}

.dark-mode .item-modal-header .modal-title,
.dark-mode .delete-modal .modal-title {
    color: #e8f5e9;
}

.dark-mode .item-modal-header .modal-title i,
.dark-mode .delete-modal .modal-title i {
    color: #00ff7f;
}

.dark-mode .form-label {
    color: var(--label-color);
}

.dark-mode .form-label i {
    color: #00ff7f;
}

.dark-mode .form-check-input {
    border-color: var(--input-border);
    background-color: var(--input-bg);
}

.dark-mode .form-check-input:checked {
    background-color: var(--accent-primary);
    border-color: var(--accent-primary);
}

.dark-mode .form-check-label i {
    /*color: var(--text-muted);*/
    color: #00ff7f !important;
    font-family: "Courier New", Courier, monospace;
    gap: 4px;
    margin-left: 2px;
}

.dark-mode .fa-toggle-on {
    color: #00ff7f !important;
}

.dark-mode .item-input,
.dark-mode .item-textarea,
.dark-mode .form-select.item-input {
    background: var(--input-bg);
    border-color: var(--input-border);
    color: var(--text-primary);
}

.dark-mode .item-input:focus,
.dark-mode .item-textarea:focus,
.dark-mode .form-select.item-input:focus {
    border-color: var(--input-focus-border);
    box-shadow: 0 0 0 0.2rem var(--input-focus-shadow);
}

.dark-mode .item-input::placeholder,
.dark-mode .item-textarea::placeholder {
    color: var(--text-muted);
}

.dark-mode .item-modal-footer,
.dark-mode .delete-modal .modal-footer {
    background: linear-gradient(90deg, #002200 0%, #004d00 50%, #002200 100%);
    border-top: 3px solid #00ff7f;
}

.dark-mode .btn-cancel {
    background: var(--btn-cancel-bg);
    border: solid 2px #e8f5e9;
    color: #e8f5e9;
}

.dark-mode .btn-cancel:hover {
    background: var(--btn-cancel-hover);
    border-color: #00ff7f;
    color: #e8f5e9;
}

.dark-mode .btn-action {
    background: linear-gradient(135deg, #002200 0%, #006400 100%);
    border-color: #00ff7f;
    color: #e8f5e9;
}

.dark-mode .btn-action:hover:not(:disabled) {
    background: linear-gradient(135deg, #006400 0%, #228b22 100%);
    border-color: #00ff7f;
    box-shadow: 0 6px 20px rgba(0, 255, 127, 0.4);
}

.dark-mode .btn-delete {
    background: #500000;
    border: 2px solid #f87171;
    color: #ffffff;
}

.dark-mode .btn-delete:hover {
    background: linear-gradient(135deg, #500000 0%, #ff3030 100%);
    border-color: #f87171;
}

.dark-mode .delete-icon-container {
    background: linear-gradient(135deg, #3a1a1a, #4a2a2a);
}

.dark-mode .delete-icon {
    color: #f87171;
}

.dark-mode .delete-title {
    color: #e8f5e9;
}

.dark-mode .delete-message {
    color: #a5d6a7;
}

.dark-mode .btn-import {
    background: linear-gradient(135deg, #0d47a1 0%, #1976d2 100%);
    border-color: #42a5f5;
}

.dark-mode .btn-import:hover {
    background: linear-gradient(135deg, #1565c0 0%, #42a5f5 100%);
    border-color: #90caf9;
}

.dark-mode .btn-export {
    background: linear-gradient(135deg, #4a148c 0%, #9c27b0 100%);
    border-color: #ba68c8;
}

.dark-mode .btn-export:hover {
    background: linear-gradient(135deg, #7b1fa2 0%, #ce93d8 100%);
    border-color: #e1bee7;
}

.dark-mode .btn-secondary,
.dark-mode .btn-secondary:focus {
    background: linear-gradient(135deg, #4a4a4a 0%, #6c757d 100%);
    color: var(--text-primary);
    border: 2px solid #e8f5e9;
    box-shadow: none;
}

.dark-mode .btn-secondary:hover:not(:disabled) {
    background: linear-gradient(135deg, #6c757d 0%, #545b62 100%);
    border-color: var(--accent-primary);
    color: var(--accent-primary);
}

.dark-mode .btn-danger {
    background: linear-gradient(135deg, #7c0000 0%, #a02020 100%);
    border: 2px solid var(--accent-danger);
    color: var(--accent-danger);
}

.dark-mode .btn-danger:hover {
    background: linear-gradient(135deg, #c62828 0%, #b91c1c 100%);
    color: #ffffff;
}

.dark-mode .item-info-preview {
    border: 1px solid #00ff7f;
    background-color: #2a3e2a !important;
}

.dark-mode .no-logo-placeholder {
    background: linear-gradient(135deg, #1b5e20 0%, #2e7d32 100%);
    border-color: #2e7d32;
    color: #00ff7f;
}

.dark-mode .current-logo-preview {
    border-color: #2e7d32;
}

.dark-mode .item-modal .modal-body::-webkit-scrollbar-thumb {
    background: #00ff7f;
}

.dark-mode .item-modal .modal-body::-webkit-scrollbar-thumb:hover {
    background: #32cd32;
}

.dark-mode .order-summary {
    background: linear-gradient(90deg, #1a2e1a 0%, #2a3e2a 100%) !important;
    border: 2px solid var(--modal-border);
    box-shadow: 0 4px 10px var(--modal-shadow);
    color: var(--text-primary);
}

.dark-mode .order-summary span,
.dark-mode .order-summary strong {
    color: var(--text-primary);
}

.dark-mode .order-summary strong#totalDisplay {
    color: var(--accent-primary) !important;
}

/* Animation */
@keyframes modalFadeIn {
    from {
        opacity: 0;
        transform: scale(0.8) translateY(-50px);
    }
    to {
        opacity: 1;
        transform: scale(1) translateY(0);
    }
}

.item-modal,
.delete-modal .modal-content {
    animation: modalFadeIn 0.3s ease-out;
}
